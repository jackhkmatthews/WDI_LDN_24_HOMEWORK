<main class="container">
  <h1>Show all quotes</h1>
  <div class="row">
    <% quotes.forEach(quote => { %>
      <div class="col-sm-6 col-md-4">
        <div class="thumbnail">
          <img src="<%= quote.imgSource %>" alt="">
          <div class="caption">
            <h3><%= quote.person %> - <%= quote.artist %></h3>
            <p><%= quote.quote %></p>
            <p><a href="/quotes/<%= quote.id %>/edit" class="btn btn-primary" role="button">Edit</a> <a href="#" class="btn btn-default" role="button">Delete</a></p>
          </div>
        </div>
      </div>
    <% }) %>
  </div>
</main>
