<main class="container">
  <div class="row">
    <div class="col-xs-12 col-md-8 col-md-offset-2">
      <h1>New</h1>
      <form action="/videos" method="post">
        <div class="form-group">
          <label for="exampleInputEmail1">Title</label>
          <input type="text" class="form-control" id="exampleInputEmail1" placeholder="Title" name="video[title]">
        </div>
        <div class="form-group">
          <label for="exampleInputEmail1">Artist</label>
          <input type="text" class="form-control" id="exampleInputEmail1" placeholder="Artist" name="video[artist]">
        </div>
        <div class="form-group">
          <label for="exampleInputPassword1">Description</label>
          <input type="text" class="form-control" id="exampleInputPassword1" placeholder="Description" name="video[description]">
        </div>
        <div class="form-group">
          <label for="exampleInputPassword1">Youtube Id</label>
          <input type="text" class="form-control" id="exampleInputPassword1" placeholder="Youtube Id" name="video[youtubeId]">
        </div>
        <button type="submit" class="btn btn-default">Create</button>
      </form>
    </div>
  </div>
</main>
